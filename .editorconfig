# Remove the line below if you want to inherit .editorconfig settings from higher directories
root = true

# JSON files
[*.json]
	indent_style = space
	indent_size = 4
	tab_width = 4
	end_of_line = lf

# C# files
[*.cs]

# Indentation and spacing
	indent_style = space
	indent_size = 4
	tab_width = 4
	end_of_line = lf

# New line preferences
	end_of_line = lf
	insert_final_newline = true
